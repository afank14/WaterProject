
@model Project

<div class="card m-1 p-1">
    <h4>@Model.ProjectName
        <span class="badge rounded-pill bg-primary text-white float-end"><small>@Model.ProjectImpact served</small></span>
    </h4>
            
    <ul>Project Type: @Model.ProjectType</ul>
    <ul>Regional Program: @Model.ProgramName</ul>
    <ul>Installation Date: @Model.ProjectInstallation</ul>
    <ul>Project Phase: @Model.ProjectPhase</ul>
</div>